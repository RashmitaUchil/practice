@page
@model Async_await.Pages.IndexModel
@using Async_await.Models

<h2>Posts</h2>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var post in Model.Posts)
        {
            <tr>
                <td>@post.Id</td>
                <td>@post.Title</td>
            </tr>
        }
    </tbody>
</table>
